<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="KERMIT"
        android:id="@+id/kermit"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_alignBottom="@+id/torgOrange"
        android:layout_toRightOf="@+id/torgOrange"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="reservTime"
        />

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="ERNA"
        android:id="@+id/erna"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_above="@+id/kermit"
        android:layout_toRightOf="@+id/torgOrange"
        android:layout_alignRight="@+id/kermit"
        android:layout_alignEnd="@+id/kermit"
        android:onClick="reservTime"
        />

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="DBASE"
        android:id="@+id/dbase"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_above="@+id/erna"
        android:layout_toRightOf="@+id/torgOrange"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="CRAY"
        android:id="@+id/cray"
        android:background="@drawable/button_white"
        android:layout_alignBottom="@+id/torgGreen"
        android:layout_toRightOf="@+id/torgGreen"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="PDP"
        android:id="@+id/pdp"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_above="@+id/cray"
        android:layout_toRightOf="@+id/torgGreen"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="VAX"
        android:id="@+id/vax"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_above="@+id/pdp"
        android:layout_toRightOf="@+id/torgGreen"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="reservTime"/>

    <Button

        android:layout_width="85dp"
        android:layout_height="120dp"
        android:text="USB"
        android:id="@+id/usb"
        android:background="@drawable/button_white"
        android:layout_alignTop="@+id/matsal"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/matsal"
        android:layout_toStartOf="@+id/matsal"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="ZELDA"
        android:id="@+id/zelda"
        android:background="@drawable/button_white"
        android:layout_alignBaseline="@+id/kermit"
        android:layout_alignBottom="@+id/kermit"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/tetris"
        android:layout_alignEnd="@+id/tetris"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="TETRIS"
        android:id="@+id/tetris"
        android:background="@drawable/button_white"
        android:layout_alignTop="@+id/erna"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/donkey"
        android:layout_alignEnd="@+id/donkey"
        android:layout_alignBottom="@+id/erna"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:text="DONKEY KONG"
        android:textSize="14sp"
        android:id="@+id/donkey"
        android:background="@drawable/button_white"
        android:layout_above="@+id/tetris"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:id="@+id/gulruta"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/zelda"
        android:layout_toEndOf="@+id/zelda"
        android:layout_marginBottom="24dp"
        />


    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:id="@+id/rodruta"
        android:background="@drawable/custom_button"
        android:layout_alignBottom="@+id/editText3"
        android:layout_alignTop="@+id/editText3"
        android:layout_toRightOf="@+id/torgBlue"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText3"
        android:text="Ledigt"
        android:layout_alignTop="@+id/gulruta"
        android:layout_toRightOf="@+id/zelda"
        android:layout_toEndOf="@+id/zelda"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText4"
        android:text=" Upptaget"
        android:layout_alignBottom="@+id/rodruta"
        android:layout_toRightOf="@+id/rodruta"
        android:layout_toEndOf="@+id/rodruta" />

    <Button
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:text="Torg Orange"
        android:id="@+id/torgOrange"
        android:layout_below="@+id/torgGreen"
        android:layout_toRightOf="@+id/torgBlue"
        android:layout_toEndOf="@+id/torgBlue" />

    <Button
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:text="Torg Grön"
        android:id="@+id/torgGreen"
        android:layout_below="@+id/router"
        android:layout_toRightOf="@+id/torgBlue"
        android:layout_toEndOf="@+id/torgBlue" />

    <Button
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:text="Torg Blå"
        android:id="@+id/torgBlue"
        android:layout_below="@+id/torgGreen"
        android:layout_toEndOf="@+id/usb"
        android:layout_toRightOf="@+id/usb" />

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="Router"
        android:id="@+id/router"
        android:background="@drawable/button_white"
        android:textColor="@color/black"
        android:layout_alignBottom="@+id/switch1"
        android:layout_toLeftOf="@+id/pdp"
        android:layout_toStartOf="@+id/pdp"
        android:layout_alignParentTop="true"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch"
        android:id="@+id/switch1"
        android:background="@drawable/button_white"
        android:textColor="@color/black"

        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/router"
        android:layout_toStartOf="@+id/router"
        android:onClick="reservTime"/>

    <Button
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:text="Matsal"
        android:id="@+id/matsal"
        android:layout_alignTop="@+id/torgGreen"
        android:layout_toEndOf="@+id/donkey"
        android:layout_toRightOf="@+id/donkey" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/refreshbutton"
        android:layout_alignBottom="@+id/router"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/refresh"
        android:background="@color/white"
        android:onClick="refreshRooms"/>


</RelativeLayout>
